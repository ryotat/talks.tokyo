(function($){
  $(document).ready(function() {
  $('<%= @target %>').talks('calendar_with_talks', [<%= @talks.map(&:start_time).map { |t| 'new Date(%d,%d,%d)'%[t.year,t.month-1,t.day] }.join(",") %>], ['<%= @talks.map {|t| link_to("#{t.name_of_speaker}: <i>#{t.title}</i>".html_safe, talk_path(t.id)).gsub('"','\"') }.join("','").html_safe %>'], '<%= @trigger %>');
});
})(jQuery);