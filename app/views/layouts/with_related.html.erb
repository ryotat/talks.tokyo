<!DOCTYPE html>
<html lang="en">
<%= render 'shared/header' %>
<body>
  <%= render 'shared/navigationbar' %>
  <div class="container-fluid">
    <%= render 'shared/breadcrumbs' %>
    <div id="flash">
      <%= show_flash %>
    </div>
    <div class="row-fluid">
      <div class="span8 main"><%= yield %></div>
      <div class="span4 sidebox"><%= render :partial => 'shared/sidebox' %></div>
    </div>
  </div>
  <%= render 'shared/footer'%>
</body>
</html>

