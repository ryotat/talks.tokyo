<div id="talks-calendar">
</div>
<% if User.current %>
<h2>Lists that you manage</h2>
<div class="btn-group-vertical">
<%= link_list User.current.personal_list, @list, 'btn' %>
<% (User.current.lists - [User.current.personal_list]).sort_by { |list| list.name }.each do |list| %>
<%= link_list list, @list, "btn list#{list.id}" %>
<% end %>
</div>
<% end %>
<%= render :partial => 'related' %>
